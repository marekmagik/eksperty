[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/marekmagik
[access "refs/for/refs/*"]
	read = group user/marekmagik
	push = group user/marekmagik
	label-Code-Review = -2..+2 group user/marekmagik
	label-Verified = -1..+1 group user/marekmagik
	rebase = group user/marekmagik
	removeReviewer = group user/marekmagik
	submit = group user/marekmagik
[access "refs/heads/*"]
	read = group user/marekmagik
	create = group user/marekmagik
	push = +force group user/marekmagik
	pushMerge = group user/marekmagik
[access "refs/tags/*"]
	push = +force group user/marekmagik
	pushTag = group user/marekmagik
	pushSignedTag = group user/marekmagik
